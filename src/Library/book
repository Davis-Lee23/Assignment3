drop table if exists Book;

create table Book
(
   bookid               bigint not null,
   bookname             varchar(255),
   state                bigint,
   authorID             bigint,
   primary key (bookid);
   foreign key (state) references islend (state) ;
   foreign key (authorID) references t_author (authorID) ;
);

